    },
    "homepage": "https://github.com/dsi-icl/borderline-ui#readme",
    "keywords": [
        "borderline",
        "bioinformatics",
        "transmart",
        "i2b2",
        "ui"
    ],
    "engines": {
        "node": ">= 7.2.0",
        "npm": ">= 4.1.0"
